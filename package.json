{
  "name": "moore-park-beach-inn",
  "version": "1.0.0",
  "description": "Moore Park Beach Inn website",
  "author": "Shaun Nielsen",
  "license": "MIT",
  "source": "src/index.html",
  "scripts": {
    "clear-docs": "rimraf docs",
    "clear-dev-srv": "rimraf dev-srv",
    "copy-site-docs": "copyfiles ./src/robots.txt ./docs/robots.txt && copyfiles ./src/sitemap.xml ./docs/sitemap.xml && copyfiles ./src/CNAME ./docs/CNAME",
    "start": "yarn run clear-dev-srv && parcel --dist-dir dev-srv",
    "build": "yarn run clear-docs && parcel build --public-url ./ --dist-dir docs && yarn run copy-site-docs"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.12.0",
    "copyfiles": "^2.4.1",
    "parcel": "^2.12.0",
    "rimraf": "^6.0.1"
  }
}
